{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "no-bitwise": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "member-ordering": false,
        "arrow-parens": false,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-separator",
            "check-type",
            "check-typecast"
        ]
    },
    "rulesDirectory": []
}
